@model IEnumerable<Domain.ContactType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ContactTypeName.MultiLangStringID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactTypeName.Value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactTypeName.Owner)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.ContactTypeName.MultiLangStringID
        </td>
        <td>
            @item.ContactTypeName.Translate()
        </td>
        <td>
            @item.ContactTypeName.Owner
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ContactTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ContactTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ContactTypeID })
        </td>
    </tr>
}

</table>
